{
  "name": "reactjs-boilerplate",
  "version": "0.0.1",
  "description": "Usages and compilation of javascript and ReactJS",
  "main": "/src/index.js",
  "scripts": {
    "check:types": "tsc --noEmit",
    "build": "yarn check:types && cross-env NODE_ENV=production webpack --config webpack.config.production.ts --mode=production",
    "start": "yarn check:types && cross-env NODE_ENV=local webpack-dev-server --config webpack.config.dev.ts --mode=development --hotOnly",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
